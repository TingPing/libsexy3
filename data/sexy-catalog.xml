<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="sexy" library="libsexy3" depends="gtk+">
  <glade-widget-classes>
    <glade-widget-class name="SexySpellEntry" generic-name="spellentry" title="SpellEntry">
        <signals>
          <signal id="word-check"/>
        </signals>
        <properties>
          <property id="checked" default="True" name="Spell check enabled"/>
